{
  "name": "update-sync-on-template-use",
  "version": "1.0.0",
  "private": true,
  "description": "An action for updating the sync.yaml file from BetaHuhn/repo-file-sync-action on template use.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "prepackage": "npm run build",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjivesterby/update-sync-on-template-use.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "template",
    "sync",
    "file-sync",
    "repo-file-sync"
  ],
  "author": "Benji Vesterby",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "json": "^11.0.0",
    "simple-git": "^2.48.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/parser": "^5.9.1",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^24.9.0",
    "jest-circus": "^27.0.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.2.4"
  }
}