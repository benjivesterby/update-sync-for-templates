{
  "name": "update-sync-for-templates",
  "version": "1.0.0",
  "private": true,
  "description": "An action for updating the sync.yaml file from BetaHuhn/repo-file-sync-action on template use.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "prepackage": "npm run build",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjivesterby/update-sync-for-templates.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "template",
    "sync",
    "file-sync",
    "repo-file-sync"
  ],
  "author": "Benji Vesterby",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "json": "^11.0.0",
    "simple-git": "^3.12.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^18.7.19",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.1",
    "jest": "^27.5.1",
    "jest-circus": "^29.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.8.3"
  }
}
